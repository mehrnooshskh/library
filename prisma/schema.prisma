// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User{
  id Int @id @default(autoincrement())
  name String? 
  userName String 
  password String
  mobile String
  email String
  createDate DateTime @default(now())
  updateDate DateTime
  status Mystatus @default(published)
}

enum Mystatus {
  published
  unpublished
}